{
  "kennungen": ["4001", "3102", "3101", "3103", "4002"],
  "trennzeichen": "-",
  "file_path": "/Users/*/.tomedoCache/temporaryFiles/proxy/*.docx",
  "gdt_path": "/Users/*/gdt_import/*.gdt",
  "export_path": "/Users/*/pdf-export",
  "delete_gdt": false,
  "delete_input": false,
  "transformations": [
    {
      "pattern": "([0-9]{2})\\.([0-9]{2})\\.([0-9]{4})",
      "replacement": "$3-$2-$1"
    },
    {
      "pattern": "([0-9]{2})([0-9]{2})([0-9]{4})",
      "replacement": "$3-$2-$1"
    },
    {
      "pattern": " ",
      "replacement": "_"
    },
    {
      "pattern": "\\.",
      "replacement": ""
    },
    {
      "pattern": "รถ",
      "replacement": "oe"
    }
  ]
}
